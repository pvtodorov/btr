                          "pair_settings" : {"shuffle": true,
                                             "seed": 47,
                                             "sample_once": false
                                            }
                         },
    "background_params": {"intervals": [{"step": 10, "start": 10, "end": 500},
                                        {"step": 100, "start": 500, "end": 1000}]
                         },
    "misc": {}
}